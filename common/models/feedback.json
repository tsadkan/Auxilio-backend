{
  "name": "Feedback",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": false
  },
  "mixins": {
    "TimeStamp": true,
    "SoftDelete": true,
    "RowCount": true
  },
  "properties": {
    "body": {
      "type": "string",
      "required": true
    },
    "files": {
      "type": [
        "object"
      ]
    },
    "container": {
      "type": "string",
      "default": "feedback"
    }
  },
  "validations": [],
  "relations": {
    "createdBy": {
      "type": "belongsTo",
      "model": "UserAccount"
    },
    "mainTopic": {
      "type": "belongsTo",
      "model": "MainTopic"
    },
    "post": {
      "type": "belongsTo",
      "model": "Post"
    },
    "replies": {
      "type": "hasMany",
      "model": "FeedbackReply"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "*"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "find"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "findById"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$admin",
      "permission": "ALLOW",
      "property": "replaceById"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$admin",
      "permission": "ALLOW",
      "property": "patchAttributes"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$admin",
      "permission": "ALLOW",
      "property": "deleteById"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "createFeedback"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "updateMyFeedback"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "deleteMyFeedback"
    }
  ],
  "methods": {}
}
